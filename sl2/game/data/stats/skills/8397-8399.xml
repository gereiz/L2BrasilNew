<?xml version="1.0" encoding="UTF-8"?>
<list>
<skill id="8397" levels="1" name="Moirai Armor Set">
		<!-- Increases P. Atk. by 4.26%, Atk. Spd. by 4%, resistance to damage magic by 2%, resistance to Stun Atk. by 50% and resistance to Fire/Water/Wind/Earth attack by 3. Cloak equip possible. STR+2. CON-2 -->
		<set name="target" val="TARGET_SELF" />
		<set name="skillType" val="BUFF" />
		<set name="operateType" val="OP_PASSIVE" />
		<for>
			<sub order="0x10" stat="CON" val="2" /> <!-- Con -2 -->
			<add order="0x10" stat="STR" val="2" /> <!-- Str +2 -->
			<mul order="0x30" stat="pAtk" val="1.0426" /> <!-- P. Atk. +4.26% -->
			<mul order="0x30" stat="pAtkSpd" val="1.04" /> <!-- Atk. Spd. +4% -->
			<sub order="0x40" stat="stunVuln" val="50" /> <!-- resistance to Stun Attacks by 50% -->
		</for>
	</skill>
	<skill id="8398" levels="1" name="Moirai Leather Armor Set">
		<!-- Increases P. Atk. by 4.26%, Atk. Spd. by 4%, Moving Spd. by 3%, resistance to Hold Atk. by 50% and resistance to Fire/Water/Wind/Earth attack by 3. Cloak equip possible. DEX+2, CON-2 -->
		<set name="target" val="TARGET_SELF" />
		<set name="skillType" val="BUFF" />
		<set name="operateType" val="OP_PASSIVE" />
		<for>
			<sub order="0x10" stat="CON" val="2" /> <!-- Con -2 -->
			<add order="0x10" stat="DEX" val="2" /> <!-- Dex +2 -->
			<mul order="0x30" stat="pAtk" val="1.0426" /> <!-- P. Atk. +4.26% -->
			<mul order="0x30" stat="pAtkSpd" val="1.04" /> <!-- Atk. Spd. +4% -->
			<sub order="0x40" stat="rootVuln" val="50" /> <!-- resistance to Hold Attacks by 50% -->
			<mul order="0x30" stat="runSpd" val="1.03" /> <!-- Moving Spd. by 3% -->
		</for>
	</skill>
	<skill id="8399" levels="1" name="Moirai Robe Set">
		<!-- Increases M. Atk. by 8.11%, Casting Spd. by 15%, resistance to Paralysis Atk. by 50% and resistance to Fire/Water/Wind/Earth attack by 5. Cloak equip possible. WIT+2, MEN-2 -->
		<set name="target" val="TARGET_SELF" />
		<set name="skillType" val="BUFF" />
		<set name="operateType" val="OP_PASSIVE" />
		<for>
			<sub order="0x10" stat="MEN" val="2" /> <!-- MEN -2 -->
			<add order="0x10" stat="WIT" val="2" /> <!-- WIT +2 -->
			<mul order="0x30" stat="mAtk" val="1.0811" /> <!-- M. Atk. +8.11% -->
			<mul order="0x30" stat="mAtkSpd" val="1.15" /> <!-- Cast. Spd. +15% -->
			<sub order="0x40" stat="paralyzeVuln" val="50" /> <!-- resistance to Paralysis Attacks by 50% -->
		</for>
	</skill>
	<skill id="8493" levels="1" name="Moirai Shield">
		<!-- Shield defense is increased. -->
		<set name="target" val="TARGET_SELF" />
		<set name="skillType" val="BUFF" />
		<set name="operateType" val="OP_PASSIVE" />
		<for>
			<mul order="0x30" stat="sDef" val="1.05" />
		</for>
	</skill>	
</list>